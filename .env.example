# Environment variables
# Copy this file to .env.local and fill in your values

# Lao Zhang API (for Nano Banana Pro image generation)
VITE_LAOZHANG_API_KEY=your_laozhang_api_key_here

# Google AI API (for caption generation using Gemini Vision)
VITE_GOOGLE_API_KEY=your_google_ai_studio_api_key

# Supabase (for image storage and database)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Make.com (for posting to social media)
MAKECOM_API_KEY=your_makecom_api_key

# Zapier (alternative)
ZAPIER_API_KEY=your_zapier_api_key

# X/Twitter API (for direct video posting)
TWITTER_CONSUMER_KEY=your_twitter_consumer_key
TWITTER_CONSUMER_SECRET=your_twitter_consumer_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret
